{
  "name": "meraki-starter",
  "version": "1.0.0",
  "description": "Meraki-starter: Config react app from scratch",
  "licenses": ["Mustafa Alroomi"],
  "main": "index.js",
  "scripts": {
    "clean-dist": "rm -f -r -d dist",
    "build": "webpack -p --env prod --config=config/prod.js",
    "start:prod": "npm run clean-dist && npm run build && webpack-dev-server",
    "start:dev": "webpack-dev-server --env dev --config=config/dev.js --open",
    "test": "jest --coverage --config=config/jest/jest.json",
    "test:w": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Mustafa Alroomi",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "jsdom": "^16.5.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
